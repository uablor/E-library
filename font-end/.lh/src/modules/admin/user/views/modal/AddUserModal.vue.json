            "content": "<template>\r\n  <div v-if=\"show\" class=\"fixed inset-0 z-50 bg-black bg-opacity-40 flex items-center justify-center\">\r\n    <div class=\"bg-white rounded-lg shadow-lg w-full max-w-2xl p-6 relative\">\r\n      <h2 class=\"text-xl font-bold mb-4\">\r\n        {{ user.code_student ? 'ແກ້ໄຂຜູ້ໃຊ້' : 'ເພີ່ມຜູ້ໃຊ້ໃໝ່' }}\r\n      </h2>\r\n\r\n      <button class=\"absolute top-2 right-2 text-gray-600 hover:text-red-500\" @click=\"$emit('close')\">✕</button>\r\n\r\n      <form @submit.prevent=\"handleSave\">\r\n        <div class=\"grid grid-cols-2 gap-4\">\r\n          <div>\r\n            <label class=\"block text-sm font-medium\">ລະຫັດນັກຮຽນ</label>\r\n            <input v-model=\"localUser.code_student\" class=\"input\" />\r\n          </div>\r\n          <div>\r\n            <label class=\"block text-sm font-medium\">ຊື່ຜູ້ໃຊ້</label>\r\n            <input v-model=\"localUser.username\" class=\"input\" />\r\n          </div>\r\n          <div>\r\n            <label class=\"block text-sm font-medium\">ອີເມວ</label>\r\n            <input v-model=\"localUser.email\" class=\"input\" />\r\n          </div>\r\n          <div>\r\n            <label class=\"block text-sm font-medium\">ຊື່ແທ້</label>\r\n            <input v-model=\"localUser.firstname\" class=\"input\" />\r\n          </div>\r\n          <div>\r\n            <label class=\"block text-sm font-medium\">ວັນເກີດ</label>\r\n            <input type=\"date\" v-model=\"localUser.birthday\" class=\"input\" />\r\n          </div>\r\n          <div>\r\n            <label class=\"block text-sm font-medium\">ທີ່ຢູ່</label>\r\n            <input v-model=\"localUser.address\" class=\"input\" />\r\n          </div>\r\n          <div>\r\n            <label class=\"block text-sm font-medium\">ພະແນກ</label>\r\n            <input v-model=\"localUser.department\" class=\"input\" />\r\n          </div>\r\n          <div>\r\n            <label class=\"block text-sm font-medium\">ເບີໂທ</label>\r\n            <input v-model=\"localUser.phone\" class=\"input\" />\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"mt-6 flex justify-end space-x-3\">\r\n          <button type=\"button\" @click=\"$emit('close')\" class=\"px-4 py-2 bg-gray-300 rounded\">ຍົກເລີກ</button>\r\n          <button type=\"submit\" class=\"px-4 py-2 bg-green-600 text-white rounded hover:bg-green-700\">ບັນທຶກ</button>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { watch, reactive, toRefs } from 'vue'\r\n\r\nconst props = defineProps({\r\n  show: Boolean,\r\n  user: Object,\r\n})\r\n\r\nconst emit = defineEmits(['close', 'save'])\r\n\r\nconst localUser = reactive({})\r\n\r\n// Watch for changes in the passed-in user prop\r\nwatch(\r\n  () => props.user,\r\n  (newUser) => {\r\n    Object.assign(localUser, newUser || {})\r\n  },\r\n  { immediate: true }\r\n)\r\n\r\nfunction handleSave() {\r\n  emit('save', { ...localUser })\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.input {\r\n  @apply mt-1 block w-full rounded border border-gray-300 px-3 py-2 shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500;\r\n}\r\n</style>\r\n"
